<?xml version='1.0' encoding='utf-8'?>
<widget id="com.quanlabs.nearme" version="2.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>nearmeApp</name>
    <description>
    </description>
    <author email="info@quanlabs.com" href="http://quanlabs.com">
        QuanLabs
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="15" />
        <icon density="mdpi" src="res/android/default/icon/48.png" />
        <icon density="hdpi" src="res/android/default/icon/72.png" />
        <icon density="xhdpi" src="res/android/default/icon/96.png" />
        <icon density="xxhdpi" src="res/android/default/icon/144.png" />
        <icon density="xxxhdpi" src="res/android/default/icon/192.png" />
        <splash density="mdpi" src="res/android/default/splash/splash-mdpi.png" />
        <splash density="hdpi" src="res/android/default/splash/splash-hdpi.png" />
        <splash density="xhdpi" src="res/android/default/splash/splash-xhdpi.png" />
        <splash density="xxhdpi" src="res/android/default/splash/splash-xxhdpi.png" />
        <splash density="xxxhdpi" src="res/android/default/splash/splash-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/ios/default/icon/Icon-60@3x.png" width="180" />
        <icon height="60" src="res/ios/default/icon/Icon-60.png" width="60" />
        <icon height="120" src="res/ios/default/icon/Icon-60@2x.png" width="120" />
        <icon height="76" src="res/ios/default/icon/Icon-76.png" width="76" />
        <icon height="152" src="res/ios/default/icon/Icon-76@2x.png" width="152" />
        <icon height="40" src="res/ios/default/icon/Icon-Small-40.png" width="40" />
        <icon height="80" src="res/ios/default/icon/Icon-Small-40@2x.png" width="80" />
        <icon height="57" src="res/ios/default/icon/Icon.png" width="57" />
        <icon height="114" src="res/ios/default/icon/Icon@2x.png" width="114" />
        <icon height="72" src="res/ios/default/icon/Icon-72.png" width="72" />
        <icon height="144" src="res/ios/default/icon/Icon-72@2x.png" width="144" />
        <icon height="29" src="res/ios/default/icon/Icon-Small.png" width="29" />
        <icon height="58" src="res/ios/default/icon/Icon-Small@2x.png" width="58" />
        <icon height="50" src="res/ios/default/icon/Icon-Small-50.png" width="50" />
        <icon height="100" src="res/ios/default/icon/Icon-Small-50@2x.png" width="100" />
        <splash height="480" src="res/ios/default/splash/Default~iphone.png" width="320" />
        <splash height="960" src="res/ios/default/splash/Default@2x~iphone.png" width="640" />
        <splash height="1024" src="res/ios/default/splash/Default-Portrait~ipad.png" width="768" />
        <splash height="2048" src="res/ios/default/splash/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="768" src="res/ios/default/splash/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="res/ios/default/splash/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1136" src="res/ios/default/splash/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/ios/default/splash/Default-667h.png" width="750" />
        <splash height="2208" src="res/ios/default/splash/Default-736h.png" width="1242" />
        <splash height="1242" src="res/ios/default/splash/Default-Landscape-736h.png" width="2208" />
    </platform>
    <engine name="ios" spec="~4.1.0" />
    <engine name="android" spec="~5.1.1" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~1.6.1" />
    <preference name="xwalkVersion" value="17+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <preference name="xwalkZOrderOnTop" value="true" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
    <plugin name="cordova-plugin-geolocation" spec="~2.1.0" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.0.11" />
    <plugin name="cordova-plugin-x-toast" spec="~2.5.0" />
    <plugin name="ionic-plugin-keyboard" spec="~2.0.1" />
    <plugin name="cordova-plugin-camera" spec="~2.1.1" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.0" />
    <plugin name="cordova-plugin-actionsheet" spec="~2.2.2" />
    <plugin name="pushwoosh-cordova-plugin" spec="~4.2.2" />
    <plugin name="cordova-plugin-google-analytics" spec="~0.8.1" />
    <plugin name="cordova-plugin-admobpro" spec="~2.14.0" />
    <plugin name="cordova-plugin-facebook4" spec="~1.7.0">
        <variable name="APP_ID" value="YOUR_FB_APP_ID" />
        <variable name="APP_NAME" value="YOUR_FB_APP_NAME" />
    </plugin>
    <plugin name="cordova-plugin-googlemaps" spec="https://github.com/phonegap-googlemaps-plugin/cordova-plugin-googlemaps">
        <variable name="API_KEY_FOR_ANDROID" value="YOUR_API_KEY_FOR_ANDROID" />
        <variable name="API_KEY_FOR_IOS" value="YOUR_API_KEY_FOR_IOS" />
    </plugin>
</widget>
